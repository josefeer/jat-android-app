            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:text="@string/NA" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:text="@string/NA" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:text="@string/NA" />
    </LinearLayout>
